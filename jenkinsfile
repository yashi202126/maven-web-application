pipeline{
    agent any
    
 tools {
  maven 'MAVEN'
  
}
    stages {
        stage('git-checkout') {
            steps {
         git credentialsId: 'github', url: 'https://github.com/kuber416/maven-web-application.git'
            } 
            
        }
         stage ('maven-build') {
            steps {
                sh "mvn clean package"
             }
        }
    }
}
